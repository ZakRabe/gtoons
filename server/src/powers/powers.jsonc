[
  {
    //TEMPLATE! DELETE AT END
    // ID of the card
    "id": -1,
    //Array of Powers applied by the card
    "powers": [
      {
        // "SINGLE" | "FOR_EACH" - if the power applies once or multiple times for each condition match
        "type": "SINGLE",
        // Conditions should be checking these special cards ONLY.
        // "NONE" | "ADJACENT" | "OPPOSING" | "NEIGHBOR" | "OPPONENT"
        "conditionRestriction": "NONE",
        // Which Card on board the power should apply to. itself, another card, neighbors, or adjacent cards
        //"SELF" | "OTHER" | "NEIGHBOR" | "ADJACENT" | "OPPOSING" | "OPPONENT"
        "target": "SELF",
        // if not target: "SELF" to be a valid target, must match ANY or ALL targetConditions
        // "ANY" | "ALL"
        "targetType": "ANY",
        // If not SELF or NEIGHBOR or ADJACENT, these conditions describe matching the target. targetConditions are same interface as regular conditions
        "targetConditions": [],
        // for this power to activate. ANY of these conditons must be true, or ALL of the conditions must be true
        // "ALL" | "ANY"
        "conditionType": "ALL",
        // Describe the things that must be TRUE for the power to be active
        "conditions": [
          {
            // "IS" | "IS_NOT"
            "condition": "IS",
            // the attribute we want to check
            "attribute": "character",
            // the value this attribute must be to qualify as a match
            "value": "Townsville",
            // ID of the CARD applying the power.
            "source": -1
          }
        ],
        // Modifier applied by the power
        // SPEICAL CASE: POINTS, REPLACE === MULTIPLY BASE POINTS.
        "modifiers": [
          { "attribute": "points", "type": "add", "value": -4, "source": -1 }
        ]
      }
    ]
  },
