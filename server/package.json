{
  "name": "gtoons-server",
  "version": "0.1.0",
  "homepage": "https://github.com/ZakRabe/gtoons",
  "description": "gtoons remake rest and websocket server",
  "main": "build/server.js",
  "scripts": {
    "build": "tsc",
    "watch": "ts-node-dev src/server.ts",
    "start": "ts-node src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate:gen": "npm run typeorm migration:generate --",
    "migrate": "npm run typeorm migration:run",
    "migrate:new": "npm run typeorm migration:create --"
  },
  "author": "zak rabe",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.3.0",
    "typeorm": "^0.2.26",
    "uuid": "^8.3.0",
    "tiny-json-http": "^7.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/helmet": "0.0.47",
    "@types/node": "^14.11.1",
    "@types/socket.io": "^2.1.11",
    "prettier": "^2.1.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  }
}
